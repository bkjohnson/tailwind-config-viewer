{
  "name": "tailwind-config-viewer",
  "version": "1.7.3",
  "description": "View your Tailwind CSS config file...visually!",
  "author": {
    "name": "Ryan Ogden",
    "email": "ryaneogden@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "build:docs": "VITE_TCG_CONFIG=tailwind.config.sample.js npm run build -- --outDir ./docs",
    "prepublishOnly": "npm run lint --no-fix && npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "test": "echo \"No test specified\"",
    "version": "npm run build:docs && git add docs"
  },
  "bin": {
    "tailwind-config-viewer": "./cli/index.js",
    "tailwindcss-config-viewer": "./cli/index.js"
  },
  "repository": "github:rogden/tailwind-config-viewer",
  "bugs": {
    "url": "https://github.com/rogden/tailwind-config-viewer/issues"
  },
  "keywords": [
    "tailwind",
    "tailwindcss"
  ],
  "license": "MIT",
  "files": [
    "server",
    "cli",
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "commander": "^6.0.0"
  },
  "peerDependencies": {
    "tailwindcss": "1 || 2 || 2.0.1-compat || 3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^10.4.17",
    "defu": "^3.2.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "postcss": "^8.4.33",
    "sass": "^1.26.10",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-draggable-resizable": "^3.0.0-beta.2"
  }
}
